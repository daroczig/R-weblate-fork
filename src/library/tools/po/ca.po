#: getfmts.c:61
#, c-format
msgid "only %d arguments are allowed"
msgstr "només es permeten %d arguments"

#: getfmts.c:66
msgid "'fmt' is not a character vector"
msgstr "«fmt» no és un vector de caràcters"

#: getfmts.c:69
msgid "'fmt' must be length 1"
msgstr "«fmt» ha de ser de longitud 1"

#: getfmts.c:75
#, c-format
msgid "'fmt' length exceeds maximal format length %d"
msgstr "la longitud de «fmt» excedeix la longitud màxima de format %d"

#: getfmts.c:96
#, c-format
msgid "unrecognised format specification '%s'"
msgstr "especificació de format «%s» no reconeguda"

#: getfmts.c:136
msgid "at most one asterisk '*' is supported in each conversion specification"
msgstr ""
"com a màxim un asterisc «*» és compatible amb cada especificació de conversió"

#: gramLatex.c:2392 gramRd.c:3995
#, c-format
msgid "unable to allocate buffer for long string at line %d"
msgstr ""
"no s'ha pogut assignar memòria intermèdia per a la cadena llarga a la línia "
"%d"

#: gramLatex.c:2629 gramRd.c:4541 gramRd.c:4547
#, c-format
msgid "invalid '%s' value"
msgstr "valor «%s» no vàlid"

#: gramRd.c:2549 gramRd.c:2552
#, c-format
msgid "bad markup (extra space?) at %s:%d:%d"
msgstr "marcatge erroni (espai extra?) a %s:%d:%d"

#: gramRd.c:3144
#, c-format
msgid "Macro '%s' previously defined."
msgstr "Macro «%s» definida anteriorment."

#: gramRd.c:3154
#, c-format
msgid "At most 4 arguments are allowed for user defined macros."
msgstr ""
"Com a màxim es permeten 4 arguments per a macros definides per l'usuari."

#: gramRd.c:3196
#, c-format
msgid "No macro definition for '%s'."
msgstr "No hi ha cap definició de macro per a «%s»."

#: gramRd.c:3258
#, c-format
msgid "Not enough arguments passed to user macro '%s'"
msgstr "No s'han passat suficients arguments a la macro d'usuari «%s»"

#: gramRd.c:3398 gramRd.c:3402
#, c-format
msgid "newline within quoted string at %s:%d"
msgstr "línia nova dins de la cadena entre cometes a %s:%d"

#: gramRd.c:3421
#, c-format
msgid "unable to allocate buffer for long macro at line %d"
msgstr ""
"no s'ha pogut assignar memòria intermèdia per a la macro llarga a la línia %d"

#: gramRd.c:3451
msgid "macros nested too deeply: infinite recursion?"
msgstr "macros aniuades massa profundament: recursivitat infinita?"

#: gramRd.c:3857
#, c-format
msgid "Unable to find macro %s"
msgstr "No s'ha pogut trobar la macro %s"

#: gramRd.c:4055
#, c-format
msgid "Unexpected end of input (in %c quoted string opened at %s:%d:%d)"
