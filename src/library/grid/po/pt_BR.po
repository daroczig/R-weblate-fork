#: gpar.c:632
msgid "updateGContext must only be called after initGContext"
msgstr "updateGContext só deve ser chamado após initGContext"

#: grid.c:199
msgid "Turning clipping on or off within a (clipping) path is no honoured"
msgstr ""
"Ativar ou desativar o recorte em um caminho (recorte) não está sendo "
"respeitado"

#: grid.c:241
msgid "cannot clip to rotated viewport"
msgstr "Não é possível cortar sobre uma viewport rotacionada"

#: grid.c:456
msgid "Clipping paths within a (clipping) path are not honoured"
msgstr ""
"O recorte dos caminhos dentro de um caminho (de recorte) não está sendo "
"respeitado"

#: grid.c:475
msgid "Masks within a (clipping) path are not honoured"
msgstr "Máscaras dentro de um caminho (recorte) não estão sendo respeitadas"

#: grid.c:699 grid.c:877
#, c-format
msgid "Viewport '%s' was not found"
msgstr "A janela de visualização gráfica '%s' não foi encontrada"

#: grid.c:909 grid.c:914 grid.c:1035 grid.c:1040
msgid "cannot pop the top-level viewport ('grid' and 'graphics' output mixed?)"
msgstr ""
"Não é possível mostrar a viewport de nível máximo (saídas gráficas e grid "
"misturadas?)"

#: grid.c:1241
msgid "attempt to plot on null device"
msgstr "tentativa de fazer um gráfico no dispositivo nulo"

#: grid.c:1711
msgid "there is no layout defined"
msgstr "Não há layout definido"

#: grid.c:1933
msgid "polygon edge not found (zero-width or zero-height?)"
msgstr "Limite do polígono não encontrado (largura ou altura zero?)"

#: grid.c:1942
msgid "polygon edge not found"
msgstr "Limite do polígono não encontrado"

#: grid.c:2096
msgid "require at least two points to draw arrow"
msgstr "Pelo menos dois pontos são necessários para desenhar uma seta"

#: grid.c:2467
msgid "non-finite control point in Xspline"
msgstr "Ponto de controle não-finito em Xspline"

#: grid.c:3436
msgid "non-finite x or y in graphics path"
msgstr "valores não-finitos de x ou y no caminho do gráfico"

#: grid.c:3487
msgid "Empty raster"
msgstr "Raster vazio"

#: grid.c:4038 grid.c:4094
msgid "Coordinates for text pch not yet supported"
msgstr "Coordenadas para 'pch' de texto ainda não suportadas"

#: grid.c:4099
#, c-format
msgid "pch value '%d' is invalid in this locale"
msgstr "O valor pch '%d' é inválido nesta configuração de localidade"

#: grid.c:4920
#, c-format
msgid "unimplemented pch value '%d'"
msgstr "O valor '%d' de pch não está implementado"

#: grid.c:5079
msgid "invalid plotting symbol"
msgstr "simbolo para gráfico inválido"

#: grid.c:5197
msgid "unable to clip to rotated rectangle"
msgstr "Não é possível cortar sobre um retângulo rotacionado"

#: layout.c:600
msgid "invalid 'layout.pos.row'"
msgstr "'layout.pos.row' inválido"

#: layout.c:604
msgid "invalid 'layout.pos.col'"
msgstr "'layout.pos.col' inválido"

#: matrix.c:50
msgid "singular transformation matrix"
msgstr "matriz de transformação singular"

#: state.c:191
msgid "unable to store 'grid' state.  Too many devices open?"
msgstr ""
"Não foi possível guardar o estado do 'grid'. Muitos dispositivos abertos?"

#: unit.c:65
msgid "Cannot create unit scalar from 0-length unit vector"
msgstr ""
"Não é possível criar escalares de unidades a partir do vetor de unidades de "
"comprimento 0"

#: unit.c:799 unit.c:1298
msgid "invalid unit or unit not yet implemented"
msgstr "Unidade inválida ou unidade ainda não implementada"

#: unit.c:1355 unit.c:1386
msgid "Viewport has zero dimension(s)"
msgstr "A janela de visualização gráfica tem dimensão zero"

#: unit.c:1418 unit.c:1433 unit.c:1448 unit.c:1463
msgid "Unsupported unit conversion"
msgstr "Conversão de unidade não suportada"

#: unit.c:1555
msgid "Invalid unit"
msgstr "Unidade inválida"

#: unit.c:1570
msgid "'units' must be character"
msgstr "'units' deve ser do tipo caractere"

#: unit.c:1573
msgid "'units' must be of length > 0"
msgstr "'units' devem ter comprimento > 0"

#: unit.c:1584
msgid "data must be either NULL, have length 1, or match the length of the final unit vector"
msgstr ""
"os dados devem ser NULL, ter comprimento 1 ou corresponder ao comprimento do "
"vetor de unidade final"

#: unit.c:1596
msgid "no string supplied for 'strwidth/height' unit"
msgstr "Nenhuma string de caracteres fornecida para a unidade 'strwidth/height'"

#: unit.c:1600
msgid "no 'grob' supplied for 'grobwidth/height' unit"
msgstr "Nenhum 'grob' fornecido para a unidade 'grobwidth/height'"

#: unit.c:1618
msgid "'gPath' must have depth 1 in 'grobwidth/height' units"
msgstr "'gPath' deve ter profundidade 1 em unidades 'grobwidth/height'"

#: unit.c:1623
msgid "non-NULL value supplied for plain unit"
msgstr "valor não NULL fornecido para unidade simples"

#: unit.c:1676 unit.c:1710 unit.c:1759
msgid "old version of unit class is no longer allowed"
msgstr "A versão antiga da classe unit não é mais permitida"

#: unit.c:1682
msgid "object is not coercible to a unit"
msgstr "objeto não é coercível a uma unidade"

#: unit.c:1708
msgid "object is not a unit"
msgstr "objeto não é uma unidade"

#: unit.c:1840
msgid "units can only be multiplied with numerics and integers"
msgstr ""
"as unidades só podem ser multiplicadas por números numéricos ou inteiros"

#: util.c:216
msgid "Unable to calculate text width/height (using zero)"
msgstr "Não foi possível calcular a largura/altura do texto (usando zero)"

#: viewport.c:56
msgid "Mask is not logical value ('none' or 'inherit')"
msgstr "Mask não é um valor lógico ('none' ou 'inherit')"

#: viewport.c:330
msgid "non-finite location and/or size for viewport"
msgstr "Localização e/ou tamanhos não finitos para viewport"

